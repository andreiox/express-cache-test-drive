{
    "name": "express-cache-test-drive",
    "version": "1.0.0",
    "description": "Express project with cache using ElastiCache for Redis",
    "main": "dist/server.js",
    "scripts": {
        "test": "ava",
        "start": "node dist/server.js",
        "build": "tsc",
        "dev": "nodemon --watch 'src/**/*.ts' --exec 'sucrase-node' src/server.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/andreiox/express-cache-test-drive.git"
    },
    "keywords": [
        "express",
        "cache",
        "redis",
        "elasticache"
    ],
    "author": "Andre Macedo",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/andreiox/express-cache-test-drive/issues"
    },
    "homepage": "https://github.com/andreiox/express-cache-test-drive#readme",
    "devDependencies": {
        "@types/cors": "^2.8.8",
        "@types/express": "^4.17.9",
        "@types/object-hash": "^1.3.4",
        "@types/redis": "^2.8.28",
        "@types/supertest": "^2.0.10",
        "ava": "^3.13.0",
        "nodemon": "^2.0.6",
        "pre-push": "^0.1.1",
        "sucrase": "^3.16.0",
        "supertest": "^6.0.1",
        "typescript": "^4.0.5"
    },
    "ava": {
        "files": [
            "!dist"
        ],
        "extensions": [
            "ts"
        ],
        "require": [
            "sucrase/register/ts"
        ]
    },
    "pre-push": {
        "silent": true,
        "run": [
            "test",
            "build"
        ]
    },
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "object-hash": "^2.0.3",
        "redis": "^3.0.2"
    }
}
